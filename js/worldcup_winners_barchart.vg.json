{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 200,
    "height": 400,
    "title": { "text": "World cup winners" },
    "data": {
        "url": "https://raw.githubusercontent.com/Dylan0209/3179/main/data/worldcups_winners.csv"
    },
    "mark": "bar",
    "encoding": {
        "y": { "field": "Count", "type": "quantitative" },
        "x": { "field": "Winner", "type": "nominal" },
        "color": {
          "field": "Winner",
          "type": "nominal",
          "legend": null,
          "scale": {
            "domain": [
              "Argentina",
              "Brazil",
              "England",
              "France",
              "Germany",
              "Italy",
              "Spain",
              "Uruguay"
            ],
            "range": [
              "#2b8cbe",
              "#fec44f",
              "#d95f0e",
              "#fee0d2",
              "#756bb1",
              "#bdbdbd",
              "#de2d26",
              "#dd1c77"
            ]
          }
        } 
    }
}